{
  "name": "corporate-dashboard",
  "version": "1.0.0",
  "description": "Udacity Corporate Dashboard Project",
  "main": "index.js",
  "scripts": {
    "_old_test": "babel-istanbul cover --report lcovonly ospec/bin/ospec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "_old_local-test": "babel-node ./ospec/bin/ospec",
    "local-test": "node_modules/.bin/babel-node node_modules/.bin/_mocha --require mocha-clean/absolute_paths --require ./tests/setup --require=env-test --require clarify --recursive ./tests --color",
    "local-test:watch": "npm run local-test -- --watch",
    "_old_cover": "babel-node _isparta cover --report lcovonly ospec/bin/ospec",
    "cover": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --require ./tests/setup --recursive ./tests",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --inline --hot --config webpack.config.dev.js",
    "lint": "eslint src",
    "start": "node ./server.js -p 1337",
    "test": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --require ./tests/setup --recursive ./tests && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js --verbose && rm -rf ./coverage",
    "server": "babel-node ./server/index.js",
    "db": "json-server ./server/db.json --static ./dist/build --port 8080 -w -S ./ --routes ./server/src/routes.json"
  },
  "standard": {
    "ignore": [
      "build",
      "app/icons",
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RichardIvan/corporate-dashboard.git"
  },
  "author": "Richard Ivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RichardIvan/corporate-dashboard/issues"
  },
  "homepage": "https://github.com/RichardIvan/corporate-dashboard#readme",
  "_comment": {
    "mithril": "github:lhorie/mithril.js#rewrite",
    "mithril-query": "github:StephanHoyer/mithril-query.git#rewrite"
  },
